document.addEventListener("DOMContentLoaded", function () {
    // Função para formatar os números de telefone
    function formatarTelefone(input, isCelular = false) {
        input.addEventListener("input", function () {
            let numero = input.value.replace(/\D/g, ""); // Remove tudo que não for número
            let formatado = "";

            // Formatação para telefone fixo (sem nono dígito)
            if (!isCelular) {
                if (numero.length <= 10) {
                    formatado = numero.replace(/(\d{2})(\d{4})(\d{4})/, "($1) $2-$3"); // (XX) XXXX-XXXX
                }
            } 
            // Formatação para telefone celular (com nono dígito)
            else {
                if (numero.length <= 11) {
                    formatado = numero.replace(/(\d{2})(\d{1})(\d{4})(\d{4})/, "($1) $2.$3-$4"); // (XX) X.XXXX-XXXX
                }
            }

            input.value = formatado;
        });
    }

    // Garantir que o script seja executado após a renderização completa da página
