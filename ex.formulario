					<script>
document.addEventListener("DOMContentLoaded", function() {
  function validarArquivo(input, botaoProximo) {
    input.addEventListener("change", function() {
      var arquivos = input.files;
      var extensoesPermitidas = ["psd", "png", "jpg", "svg", "pdf", "zip", "rar"];
      var extensoesVetoriais = ["ai", "eps", "cdr"];
      var tamanhoMaximo = 10 * 1024 * 1024; // 10MB
      
      var invalido = false,
          precisaCompactar = false,
          tamanhoExcedido = false;
      
      // Se nenhum arquivo for selecionado, mantém o botão bloqueado
      if (arquivos.length === 0) {
        botaoProximo.disabled = true;
        return;
      }
      
      // Verifica cada arquivo selecionado
      for (var i = 0; i < arquivos.length; i++) {
        var arquivo = arquivos[i];
        var nomeArquivo = arquivo.name.toLowerCase();
        var tamanhoArquivo = arquivo.size;
        var extensaoArquivo = nomeArquivo.split(".").pop();
        
        // Extensão não permitida
        if (!extensoesPermitidas.includes(extensaoArquivo) && !extensoesVetoriais.includes(extensaoArquivo)) {
          invalido = true;
        }
        // Arquivos vetoriais devem ser compactados
        if (extensoesVetoriais.includes(extensaoArquivo)) {
          precisaCompactar = true;
        }
        // Tamanho excedido
        if (tamanhoArquivo > tamanhoMaximo) {
          tamanhoExcedido = true;
        }
      }
      
      // Seleciona ou cria o elemento para exibir a mensagem de erro
      var erroMensagem = input.parentNode.querySelector(".elementor-message");
      if (!erroMensagem) {
        erroMensagem = document.createElement("div");
        erroMensagem.classList.add("elementor-message", "elementor-message-danger");
        erroMensagem.style.marginTop = "5px";
        input.parentNode.appendChild(erroMensagem);
      }
      
      // Validação e exibição das mensagens de erro
      if (invalido) {
        erroMensagem.textContent = "O arquivo enviado não é permitido. Use: PSD, PNG, JPG, SVG, PDF. Para AI, EPS, CDR, compacte em ZIP ou RAR.";
        erroMensagem.style.display = "block";
        botaoProximo.disabled = true;
        return;
      }
      
      if (precisaCompactar) {
        erroMensagem.textContent = "Arquivos AI, EPS, CDR devem ser compactados em ZIP ou RAR.";
        erroMensagem.style.display = "block";
        botaoProximo.disabled = true;
        return;
      }
      
      if (tamanhoExcedido) {
        erroMensagem.textContent = "O arquivo excede o limite de 10MB.";
        erroMensagem.style.display = "block";
        botaoProximo.disabled = true;
        return;
      }
      
      // Se tudo estiver correto, oculta a mensagem de erro e habilita o botão "Próximo"
      erroMensagem.style.display = "none";
      botaoProximo.disabled = false;
    });
  }
  
  function monitorarCampoUpload() {
    // Seleciona o campo de upload e o botão "Próximo"
    var campoArquivo = document.querySelector("#field_3018685");
    var botaoProximo = document.querySelector(".e-form__buttons__wrapper__button-next");
    
    if (!campoArquivo) {
      console.error("Campo de upload #field_3018685 não encontrado.");
      return;
    }
    if (!botaoProximo) {
      console.error("Botão 'Próximo' não encontrado.");
      return;
    }
    
    // Inicialmente, bloqueia o botão "Próximo"
    botaoProximo.disabled = true;
    
    // Aplica a validação ao campo de upload
    validarArquivo(campoArquivo, botaoProximo);
  }
  
  // Aguarda 1 segundo para garantir que os elementos estejam carregados (compatível com Elementor)
  setTimeout(monitorarCampoUpload, 1000);
});
</script>
