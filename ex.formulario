document.addEventListener("DOMContentLoaded", function () {
    function formatarCEP(input) {
        input.addEventListener("input", function () {
            let cep = input.value.replace(/\D/g, ""); // Remove tudo que não for número
            if (cep.length > 8) cep = cep.substring(0, 8); // Limita a 8 dígitos
            if (cep.length === 8) {
                input.value = cep.replace(/(\d{5})(\d{3})/, "$1-$2"); // Formato XXXXX-XXX
                buscarCEP(cep);
            } else {
                input.value = cep;
            }
        });

        input.addEventListener("keypress", function (event) {
            if (event.key === "Enter") {
                let cep = input.value.replace(/\D/g, "");
                if (cep.length === 8) {
                    event.preventDefault();
                    buscarCEP(cep);
                }
            }
        });
    }

    function buscarCEP(cep) {
        fetch(`https://viacep.com.br/ws/${cep}/json/`)
            .then(response => response.json())
            .then(data => {
                if (!data.erro) {
                    preencherCampos(data);
                } else {
                    alert("CEP não encontrado!");
                }
            })
            .catch(error => console.error("Erro na requisição:", error));
    }

    function preencherCampos(data) {
        document.querySelector("input[name='form_fields[field_d56f995]']").value = data.logradouro || "";
        document.querySelector("input[name='form_fields[field_f51f6f4]']").value = data.bairro || "";
        document.querySelector("input[name='form_fields[field_16b15c1]']").value = data.localidade || "";

        let estadoSelect = document.querySelector("select[name='form_fields[fiaeld_0d030bf]']");
        if (estadoSelect) {
            let options = estadoSelect.options;
            for (let i = 0; i < options.length; i++) {
                if (options[i].value.toLowerCase() === data.uf.toLowerCase()) {
                    estadoSelect.value = options[i].value;
                    break;
                }
            }
        }
    }

    setTimeout(function() {
        let cepField = document.querySelector("input[name='form_fields[field_f6e0d3e]']");
        if (cepField) {
            formatarCEP(cepField);
        }
    }, 1000); // Delay para garantir que o DOM esteja carregado
});
</script>
